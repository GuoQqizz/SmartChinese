@model AbhsChinese.School.Models.CashVoucher.CashVoucherSearch
<form role="form" class="form-inline search-form col-10 col-md-9" id="form_search">
    <input type="hidden" name="SchoolId" value="@Model.SchoolId"  />
    <div class="form-group">
        <input type="text" class="form-control" name="Id" placeholder="按编号搜索" />
    </div>
    <div class="form-group">
        <input type="text" class="form-control" name="Name" placeholder="按名称搜索" />
    </div>
    <div class="form-group">
        <select class="form-control m-b select2 select2-cash-status" name="Status"></select>
    </div>
    <button class="btn btn-white search" type="button">
        搜索
    </button>
</form>
