<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <section name="redis" type="AbhsChinese.Code.Cache.RedisConfigInfoSection,AbhsChinese.Code" />
  </configSections>
  <redis MaxWritePoolSize="20" MaxReadPoolSize="20" AutoStart="true" WriteServerList="192.168.3.242" ReadServerList="192.168.3.242" />
  <connectionStrings configSource="Configs\database.config" />

  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    
    <!--OSS上传Url-->
    <add key="uploadUrl" value="http://192.168.3.242:8089" />
    <!--OSS下载地址根路径-->
    <add key="OssHostUrl" value="http://abhstest.oss-cn-beijing.aliyuncs.com" />
    <!--OSS上传是subject参数-->
    <add key="OssSubject" value="abhstest" />
    
    <!--题库图片的顶级目录-->
    <add key="TopDirectoryOfSubject" value="QuestionDatabase" />

    <!--百度语音合成-->
    <add key="BaiduAccessToken" value="https://aip.baidubce.com/oauth/2.0/token" />
    <add key="TextToAudioAppId" value="x2iNTQ8o4dwRomcmg2lXHg2K" />
    <add key="TextToAudioSecret" value="NwcLSXxWRR42ZRiKjKw4tumn3e8pKzxG" />
    <add key="TextToAudioCUID" value="abhstext" />
    <add key="TextToAudioURL" value="http://tsn.baidu.com/text2audio" />
    <add key="Language" value="zh" />
    <add key="Speed" value="5" />
    <add key="Pitch" value="5" />
    <add key="Volume" value="7" />
    <add key="Pronunciation" value="4" />
        
    <!--学生端ip-->
    <add key="StudentClientDomain" value="https://192.168.3.161:443/" />
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.6.1" />
    <httpRuntime targetFramework="4.6.1" maxRequestLength="31457280" />
    <httpModules>
      <add name="ApplicationInsightsWebTracking" type="Microsoft.ApplicationInsights.Web.ApplicationInsightsHttpModule, Microsoft.AI.Web" />
    </httpModules>
  </system.web>
  <system.webServer>
    <security>
      <requestFiltering>
        <requestLimits maxAllowedContentLength="31457280"></requestLimits>
      </requestFiltering>
    </security>
  </system.webServer>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" culture="neutral" publicKeyToken="30ad4fe6b2a6aeed" />
        <bindingRedirect oldVersion="0.0.0.0-12.0.0.0" newVersion="12.0.0.0" />
