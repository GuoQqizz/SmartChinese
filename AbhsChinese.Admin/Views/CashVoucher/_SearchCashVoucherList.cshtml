<form role="form" class="form-inline search-form" style="margin-top:0px;" id="form_search_cash">
    <input type="hidden" name="cashType" value="" />
    <div class="form-group">
        <input type="text" class="form-control" name="Id" placeholder="按编号搜索" />
    </div>
    <div class="form-group">
        <input type="text" class="form-control" name="Name" placeholder="按名称搜索" />
    </div>
    <div class="form-group">
        <select class="select2 form-control" data-ajax--url="@Url.Action("GetVoucherStatus","Select2")"
                data-ajax--cache="true"
                data-placeholder="全部状态" data-allow-clear="true"
                name="Status">
            <option></option>
        </select>
    </div>
    <button class="btn btn-white search" type="button">
        搜索
    </button>
</form>
